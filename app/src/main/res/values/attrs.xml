<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundImageView">
        <attr name="type" format="enum">
            <enum name="circle" value="1"/>
            <enum name="round" value="2"/>
        </attr>
        <attr name="radius" format="dimension"/>ssss
    </declare-styleable>
    <declare-styleable name="MusicDrawerItemView">
        <attr name="item_icon" format="reference" />
        <attr name="item_name" format="reference" />
        <attr name="is_show_gap" format="boolean" />
    </declare-styleable>
</resources>